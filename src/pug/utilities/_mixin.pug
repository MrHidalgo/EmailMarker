//- btn with arrow
//- =========================
mixin btn(classMod, val)
  a.c-btn(href="#" class="c-btn--" + classMod title="")
    .c-btn__left
      span!= val
    .c-btn__right
      i.icon-font.icon-arrow-right
//- =========================
//- btn with arrow :: end


